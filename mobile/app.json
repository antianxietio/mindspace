{
  "expo": {
    "name": "MindSpace",
    "slug": "mindspace",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindspace.app",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes for session check-in/check-out."
      }
    },
    "android": {
      "package": "com.mindspace.app",
      "permissions": ["CAMERA"],
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "apiUrl": "https://cfb884d6ed8b.ngrok-free.app/api"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
